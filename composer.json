{
    "name": "consultaazul/pt-br-validator",
    "description": "Biblioteca para validação de formatos brasileiros. Baseado no laravellegends/pt-br-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Wallace de Souza Vizerra",
            "email": "wallacemaxters@gmail.com"
        },
        {
            "name": "Consulta Azul",
            "email": "development@consultaazul.com.br"
        }
    ],
    "require": {
        "illuminate/support": "^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "ConsultaAzul\\PtBrValidator\\": "src/pt-br-validator"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConsultaAzul\\PtBrValidator\\ValidatorProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.3 || ^9.0"
    }
}
